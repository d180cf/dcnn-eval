{
  "version": "1.0.0",
  "author": "d180cf",
  "dependencies": {
    "command-line-args": "4.0.7",
    "sgf-problems": "https://github.com/d180cf/problems#v2.0.1",
    "tsumego.js": "1.0.4",
    "md5": "2.2.1",
    "glob": "7.1.2",
    "mkdirp": "0.5.1",
    "@types/node": "8.0.46"
  },
  "scripts": {
    "solve": "node solve node_modules/sgf-problems/gop/12354.sgf .bin/test/vars.sgf 20 7",
    "vplay": "node vplay .bin/test/vars.sgf .bin/test/subp",    
    "feats": "node -e \"require('./feats).compute('.bin/subproblems/62612cd.sgf', '.bin/features')\"",
    "fview": "node fview .bin/features.json .bin/features.txt",

    "solve-all": "node solve-all .bin/variations 20 7",
    "vplay-all": "node vplay-all .bin/variations/*.sgf .bin/subproblems",
    "check-all": "node check-all .bin/subproblems/*.sgf 0.001",
    "feats-all": "node feats-all .bin/subproblems/*.sgf .bin/features",

    "pack-problems": "node pack .bin/subproblems/*.sgf .bin/subproblems.log",
    "pack-features": "node pack .bin/features/*.json .bin/features.log",

    "stats": "node stats .bin/subproblems/*.sgf",

    "dcnn": "py dcnn.py"
  }
}
