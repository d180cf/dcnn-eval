{
  "version": "1.0.0",
  "author": "d180cf",
  "dependencies": {
    "command-line-args": "4.0.7",
    "sgf-problems": "https://github.com/d180cf/problems#v2.0.1",
    "tsumego.js": "1.0.4",
    "md5": "2.2.1",
    "glob": "7.1.2",
    "mkdirp": "0.5.1",
    "@types/node": "8.0.46"
  },
  "scripts": {
    "solve": "node solve node_modules/sgf-problems/gop/12354.sgf .bin/test/vars.sgf 20 7",
    "vplay": "node vplay .bin/test/vars.sgf .bin/test/subp",    
    "feats": "node -e \"require('./feats).compute('.bin/problems/62612cd.sgf', '.bin/features')\"",
    "fview": "node fview .bin/features.json .bin/features.txt",

    "solve-all": "node solve-all .bin/solutions 20 7",
    "vplay-all": "node vplay-all .bin/solutions/*.sgf .bin/problems",
    "validate": "node check-all .bin/problems/*.sgf 0.001",
    "features": "node feats-all .bin/problems/*.sgf .bin/features",

    "pack-problems": "node pack .bin/problems/*.sgf .bin/problems.log",
    "pack-features": "node pack .bin/features/*.json .bin/features.log",

    "stats": "node stats .bin/problems/*.sgf",

    "make-tfrs": "python make-tfrs.py .bin/features .bin/main.tfrecords .bin/test.tfrecords 0.1 7",
    "test-tfrs": "python test-tfrs.py .bin/test.tfrecords 10",
    "read-tfrs": "python read-tfrs.py .bin/test.tfrecords 9",

    "watch": "start tensorboard --logdir=.bin/tf-logs",
    "dcnn": "py dcnn.py .bin/main.tfrecords .bin/test.tfrecords 11 5 .bin/tf-model.json .bin/tf-err.log .bin/tf-logs"
  }
}
