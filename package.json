{
  "version": "1.0.0",
  "author": "d180cf",
  "dependencies": {
    "command-line-args": "4.0.7",
    "sgf-problems": "https://github.com/d180cf/problems#v2.0.1",
    "tsumego.js": "1.1.0",
    "md5": "2.2.1",
    "glob": "7.1.2",
    "mkdirp": "0.5.1",
    "@types/node": "8.0.46"
  },
  "scripts": {
    "solve": "node js/solve node_modules/sgf-problems/gop/12354.sgf .bin/test/vars.sgf 20 7",
    "vplay": "node js/vplay .bin/test/vars.sgf .bin/test/subp",    
    "feats": "node -e \"require('./js/feats).compute('.bin/problems/62612cd.sgf', '.bin/features')\"",
    "fview": "node js/fview .bin/features.json .bin/features.txt",

    "solve-all": "node js/solve-all .bin/solutions 20 7",
    "vplay-all": "node js/vplay-all .bin/solutions/*.sgf .bin/problems",
    "validate": "node js/check-all .bin/problems/*.sgf 0.001",
    "features": "node js/feats-all .bin/problems/*.sgf .bin/features",

    "pack-problems": "node js/pack .bin/problems/*.sgf .bin/problems.log",
    "pack-features": "node js/pack .bin/features/*.json .bin/features.log",

    "stats": "node js/stats .bin/problems/*.sgf",

    "tfrecords": "python py/make-tfrs.py .bin/features .bin/main.tfrecords .bin/test.tfrecords 0.1",
    "test-tfrs": "python py/test-tfrs.py .bin/test.tfrecords 10",
    "read-tfrs": "python py/read-tfrs.py .bin/test.tfrecords 9",

    "tensorboard": "start tensorboard --logdir=.bin/tf-logs",
    "dcnn": "python py/dcnn.py .bin/main.tfrecords .bin/test.tfrecords 11 13 .bin/tf-models .bin/tf-logs 15 rb1(1,256)",
    "eval": "node js/eval-dcnn .bin/tf-models/rb1-1-256-snapshot.json .bin/problems/*77.sgf"
  }
}
